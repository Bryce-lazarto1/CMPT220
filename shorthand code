/// create an array of your friends names. Print them out using a for loop and then a for-each loop///
class Main {
    public static void main(String[] args) {
            String[] friends = {"bryce", "john", "carter", "christian", "daniel"};
             for (String s : friends) {
                System.out.println(friends[s]); }
            System.out.println();
            for (int i = 0; i < friends.length; i++) {
                System.out.println(i);
        }
    }
